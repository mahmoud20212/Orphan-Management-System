   <string notr="true">/* =================================================
   GLOBAL
================================================= */
QWidget {
    background-color: #F4F6F9;
    color: #1F2933;
    font-family: &quot;Segoe UI&quot;;
}

/* =================================================
   TOOLTIP
================================================= */
QToolTip {
    background-color: #FFFFFF;
    color: #1F2933;
    border: 1px solid #CBD2D9;
    border-radius: 6px;
    padding: 6px;
}

/* =================================================
   INPUTS
================================================= */
QLineEdit,
QTextEdit,
QPlainTextEdit,
QComboBox,
QSpinBox,
QDoubleSpinBox,
QDateEdit,
QDateTimeEdit {
    background-color: #FFFFFF;
    border: 1px solid #CBD2D9;
    border-radius: 4px;
    padding: 6px;
}

QLineEdit:focus,
QTextEdit:focus,
QComboBox:focus,
QDateEdit:focus,
QDateTimeEdit:focus {
    border: 1px solid #2563EB;
}

/* =================================================
   BUTTONS
================================================= */
QPushButton {
    background-color: #2563EB;
    color: #FFFFFF;
    border: none;
    border-radius: 4px;
    padding: 8px 18px;
    font-weight: 500;
}

QPushButton:hover {
    background-color: #1D4ED8;
}

QPushButton:pressed {
    background-color: #1E40AF;
}

QPushButton:disabled {
    background-color: #E5E7EB;
    color: #9CA3AF;
}

/* =================================================
   ITEM VIEWS
================================================= */
QTableWidget,
QTreeWidget,
QListView,
QAbstractItemView {
    background-color: #FFFFFF;
    border: 1px solid #CBD2D9;
    gridline-color: #E5E7EB;
    alternate-background-color: #F9FAFB;
}

QAbstractItemView::item:hover {
    background-color: #EEF2FF;
}

QAbstractItemView::item:selected {
    background-color: #2563EB;
    color: white;
}

/* =================================================
   HEADERS
================================================= */
QHeaderView::section {
    background-color: #E5E7EB;
    color: #1F2933;
    padding: 10px;
    border: none;
    font-weight: bold;
}

/* =================================================
   MENUS
================================================= */
QMenuBar {
    background-color: #FFFFFF;
    border-bottom: 1px solid #CBD2D9;
}

QMenuBar::item:selected {
    background-color: #EEF2FF;
    border-radius: 6px;
}

QMenu {
    background-color: #FFFFFF;
    border: 1px solid #CBD2D9;
}

QMenu::item:selected {
    background-color: #EEF2FF;
}

/* =================================================
   TABS
================================================= */
QTabWidget::pane {
    border: 1px solid #CBD2D9;
    border-radius: 6px;
}

QTabBar::tab {
    background-color: #E5E7EB;
    padding: 8px 18px;
    border-radius: 6px;
    margin-right: 4px;
}

QTabBar::tab:selected {
    background-color: #2563EB;
    color: white;
}

/* =================================================
   GROUP BOX
================================================= */
QGroupBox {
    border: 1px solid #CBD2D9;
    border-radius: 8px;
    margin-top: 12px;
}

QGroupBox::title {
    color: #2563EB;
    font-weight: bold;
    left: 10px;
}

/* =================================================
   CHECKBOX &amp; RADIO
================================================= */
QCheckBox::indicator,
QRadioButton::indicator {
    width: 14px;
    height: 14px;
    border: 1px solid #9CA3AF;
    background-color: white;
}

QCheckBox::indicator:checked,
QRadioButton::indicator:checked {
    background-color: #2563EB;
}

/* =================================================
   SLIDER
================================================= */
QSlider::groove:horizontal {
    background: #E5E7EB;
    height: 6px;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #2563EB;
    width: 14px;
    border-radius: 7px;
}

/* =================================================
   PROGRESS BAR
================================================= */
QProgressBar {
    background-color: #E5E7EB;
    border-radius: 6px;
    text-align: center;
}

QProgressBar::chunk {
    background-color: #2563EB;
}

/* =================================================
   SCROLLBAR
================================================= */
QScrollBar:vertical {
    background: #F4F6F9;
    width: 10px;
}

QScrollBar::handle:vertical {
    background: #CBD2D9;
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover {
    background: #9CA3AF;
}

/* =================================================
   DISABLED
================================================= */
QWidget:disabled {
    color: #6c757d;             /* لون النص مثل Bootstrap */
    background-color: #e9ecef;  /* خلفية فاتحة */
    border: 1px solid #ced4da;  /* إطار خفيف */
}
/* الزر الطبيعي */
#pushButton_3,
#pushButton_delete_row,
#pushButton_guardian_delete,
#pushButton_delete_row_3 {
    background-color: #dc3545;  /* أحمر Bootstrap */
    color: white;
    border: 1px solid #dc3545;
    border-radius: 4px;
    padding: 5px 12px;
}

/* عند المرور بالماوس */
#pushButton_3:hover,
#pushButton_delete_row:hover,
#pushButton_guardian_delete:hover,
#pushButton_delete_row_3:hover {
    background-color: #c82333;
    border: 1px solid #c82333;
}

/* عند التعطيل */
#pushButton_3:disabled,
#pushButton_guardian_delete:disabled {
    background-color: #e0e0e0;  /* رمادي فاتح */
    color: #6c757d;              /* نص غامق */
    border: 1px solid #d6d6d6;
}

#lineEdit_8,
#lineEdit_9,
#comboBox_3,
#dateEdit_3 {
	font-size: 12px;
}

#tableWidget_4 {
	font-size: 12px;
}

QGroupBox {
    background-color: #ffffff; /* خلفية بيضاء */
}

#frame,
#frame_2,
#frame_3,
#frame_4 {
    background-color: #ffffff; /* خلفية بيضاء */
    border-radius: 10px;       /* زوايا مستديرة */
    border: 1px solid #d1d5db; /* حدود خفيفة */
}

QPushButton#export_report,
QPushButton#export_report_2,
QPushButton#export_report_3,
QPushButton#monthly_report {
    background-color: #198754;      /* Bootstrap success */
    color: white;
    border: 1px solid #198754;
    border-radius: 4px;
    padding: 6px 14px;
}

QPushButton#export_report:hover,
QPushButton#export_report_2:hover,
QPushButton#export_report_3:hover,
QPushButton#monthly_report:hover {
    background-color: #157347;
    border-color: #157347;
}

QPushButton#export_report:pressed,
QPushButton#export_report_2:pressed,
QPushButton#export_report_3:pressed,
QPushButton#monthly_report:pressed {
    background-color: #146c43;
    border-color: #146c43;
}

QPushButton#export_report:disabled,
QPushButton#export_report_2:disabled,
QPushButton#export_report_3:disabled,
QPushButton#monthly_report:disabled {
    background-color: #a3cfbb;
    border-color: #a3cfbb;
    color: #ffffff;
}
